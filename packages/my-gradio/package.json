{
  "name": "my-gradio",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "../components/dist/*.js",
      "types": "../components/dist/*.d.ts"
    },
    "./style.css": "./src/styles.css"
  },
  "dependencies": {
    "@my-gradio/api": "workspace:*",
    "@my-gradio/components": "workspace:*",
    "@my-gradio/core": "workspace:*",
    "@my-gradio/layout": "workspace:*",
    "@my-gradio/state": "workspace:*",
    "@my-gradio/theme": "workspace:*"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  }
}
